{
  "name": "CrateMate",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/api": {
      "target": "<url>",
      "ws": true
    }
  },
  "dependencies": {
    "async-middleware": "^1.2.1",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "bulma": "^0.5.3",
    "express": "^4.16.1",
    "method-override": "^2.3.10",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "sequelize": "^4.13.15"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npx babel api --out-dir dist",
    "build-babel": "npx babel api/server.js --out-file server.js && npx babel api --out-dir dist",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
